{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    
    "plugins": [
        "react"
    ],
    "rules": {
        "ignorePatterns": [
            "**/webpack.config.js",
            "**/webpack.dev.js",
            "**/webpack.prod.js"
          ]
    }
}
