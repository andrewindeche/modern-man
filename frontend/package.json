{
  "name": "frontend",
  "version": "1.0.0",
  "engine": {
    "version": "v21.4.0"
  },
  "description": "A React App for modernman",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --open --port 3000 --config webpack.prod.js",
    "lint": "eslint",
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "clean": "rm -rf dist/",
    "dev": "webpack serve --open --port 3000 --config webpack.dev.js"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.(ico|svg|jpeg|jpg|png|gif|webp)$": "jest-transform-stub"
    }
  },
  "author": "Andrew Indeche",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^29.7.0",
    "compression-webpack-plugin": "^11.1.0",
    "css-loader": "^6.10.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-transform-stub": "^2.0.0",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "style-loader": "^3.3.4",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "dependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "babel-loader": "^9.1.3",
    "babel-preset": "^1.1.7",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^5.3.3",
    "env": "^0.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.3",
    "webpack-merge": "^5.10.0"
  }
